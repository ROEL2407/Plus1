@model Plus1.Models.CategoryViewModel


@{
    ViewBag.Title = "Details";
}

<div>
    <div class="brands backgrnd_cat">
        <div class="container">
            @foreach (var item in Model.SubCategory)
            {
            <h3>@item.Name</h3>
            }

            <div class="brands-agile">
                @foreach (var item in Model.SubSubCategory)
                {
                <div class="col-md-4 w3layouts-brand">
                    <div class="brands-w3l">
                        <img src="/content/uploads/@Html.DisplayFor(modelItem => item.Image)" alt="" class="img-responsive" />
                        <p>
                            @Html.ActionLink(item.Name, "Details", "SubSubCategory", new { id = item.Name }, null)
                        </p>
                    </div>
                </div>
                }
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
</div>
