@model IEnumerable<Plus1.Models.Category>

@{
    ViewBag.Title = "Plus1";
}
<div class="brands backgrnd_cat">
    <div class="container">
        <h3>Categorieën</h3>


        <div class="brands-agile">
            @foreach (var item in Model)
            {
        <div class="col-md-4 w3layouts-brand">
            <div class="brands-w3l">
                <img src="content/uploads/@Html.DisplayFor(modelItem => item.Image)" alt="" class="img-responsive" />

                <p>@Html.ActionLink(item.Name, "Details", new { id = item.Name }, htmlAttributes: new { id = "koko" })</p>
            </div>
        </div>
            }
        <div class="clearfix"></div>
        </div>
    </div>
</div>
    